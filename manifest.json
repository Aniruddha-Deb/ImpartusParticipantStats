{
	"name": "Moodle Sidebar Plugin",
	"version": "1.0",
	"description": "Changes Moodle Sidebar Content",
	"content_scripts": [ {
		"matches": [
			"https://moodle.iitd.ac.in/my/", 
			"https://moodle.iitd.ac.in/course/*",
			"https://moodle.iitd.ac.in/mod/*",
			"https://moodle.iitd.ac.in/grade/*",
			"https://moodle.iitd.ac.in/user/*"
		],
		"run_at": "document_idle",
		"js": ["plugin.js"]
	} ],
	"icons": {
		"36": "icons/icon36.png",
		"48": "icons/icon48.png",
		"96": "icons/icon96.png",
		"192": "icons/icon192.png",
		"512": "icons/icon512.png"
	},
	"manifest_version": 2
}
